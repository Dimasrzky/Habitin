{
  "expo": {
    "name": "Habitin",
    "slug": "Habitin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo_Lauch.png",
    "scheme": "habitin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ECF4E8",
        "foregroundImage": "./assets/images/Logo_Lauch.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.Habitin",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo_Lauch.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ECF4E8",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
